<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.ravneet.ieeedtu.PrivateActivity.EventNotification">

    <include
        layout="@layout/include_ieee_header"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:gravity="center"
        android:textColor="@android:color/black"

        android:text="EVENT Notification"/>
    <Button
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="POST Notification For Sig"
    android:id="@+id/btn_PostSigNotification"
    android:layout_gravity="bottom"/>

</LinearLayout>
